{
    "entryPoints": [
        "./src/OSFramework/DataGrid/**/*.ts",
        "./src/OutSystems/OutSystems.ts",
        "./src/Providers/DataGrid/**/*.ts"
    ],
    "entryPointStrategy": "expand",
    "includeVersion": true,
    "hideGenerator": true,
    "out": "./docs",
    "sort": [
        "source-order"
    ],
    "umlClassDiagram": {
        "type": "detailed",
        "location": "local",
        "format": "svg",
        "backgroundColor": "#ced4da",
        "boxBackgroundColor": "#fff",
        "boxBorderColor": "#272b30",
        "arrowColor": "#272b30"
    },
    "customCss": "typedoc-custom.css",
}
