{
    "entryPoints": [
        "./src/OSFramework/DataGrid//*.ts",
        "./src/OutSystems/DataAPI//*.ts",
        "./src/Providers/DataGrid//*.ts"
    ],
    "entryPointStrategy": "expand",
    "includeVersion": true,
    "hideGenerator": true,
    "out": "./docs",
    "sort": [
        "source-order"
    ],
    "umlClassDiagram": {
